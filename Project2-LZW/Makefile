
target:
	@javac ./MyLZW.java
	@javac ./LZW.java
run:
	@java MyLZW - r < code.txt > codeR.lzw
	@java MyLZW +  < codeR.lzw > codeR.txt
	@java MyLZW - m < code.txt > codeM.lzw
	@java MyLZW +  < codeM.lzw > codeM.txt
	@java MyLZW - n < code.txt > codeN.lzw
	@java MyLZW +  < codeN.lzw > codeN.txt
	@java LZW - < code.txt > codeO.lzw
	@java LZW +  < codeO.lzw > codeO.txt

	@java MyLZW - r < all.tar > allR.lzw
	@java MyLZW +  < allR.lzw > allR.tar
	@java MyLZW - m < all.tar > allM.lzw
	@java MyLZW +  < allM.lzw > allM.tar
	@java MyLZW - n < all.tar > allN.lzw
	@java MyLZW +  < allN.lzw > allN.tar
	@java LZW - < all.tar > allO.lzw
	@java LZW +  < allO.lzw > allO.tar

	@java MyLZW - r < assig2.doc > assig2R.lzw
	@java MyLZW +  < assig2R.lzw > assig2R.doc
	@java MyLZW - m < assig2.doc > assig2M.lzw
	@java MyLZW +  < assig2M.lzw > assig2M.doc
