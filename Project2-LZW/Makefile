clean:
	@ -rm *.class
	@ -rm *.lzw
	@ -rm *.bmp
	@ -rm *.tar
	@ -rm *.gif
	@ -rm *.txt
	@ -rm *.doc
	@ -rm *.exe
	@ -rm *.jpg
